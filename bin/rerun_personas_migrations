#!/usr/bin/env bash
set -eux
bundle exec rails db:pg:terminate_connections db:drop db:create db:migrate VERSION=340
bundle exec rails db:pg:truncate_tables db:pg:data:restore FILE=db/personas.340.pgbin
bundle exec rails db:migrate
bundle exec rails db:seed
rm -f db/personas.pgbin
bundle exec rails db:pg:structure_and_data:dump FILE=db/personas.pgbin
